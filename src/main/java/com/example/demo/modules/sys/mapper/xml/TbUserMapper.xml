<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.modules.sys.mapper.TbUserMapper">

    <select id="selectByUsername" resultType="com.example.demo.modules.sys.entity.TbUser">
        select * from tb_user
        where username = #{username} limit 1
    </select>

</mapper>
